{
  "name": "astronati/fantasy-football-quotations-parser",
  "description": "This library allows user to retrieve quotations from those documents that are provided from the most important sport newspapers. Quotations contains votes of each soccer player of a specific championship match day.",
  "license": "MIT",
  "version": "2.4.3",
  "authors": [
    {
      "name": "Andrea Stronati",
      "email": "astronati@vendini.com"
    }
  ],
  "minimum-stability": "stable",
  "require-dev": {
    "codacy/coverage": "^1.4",
    "consolidation/robo": "~1",
    "phpunit/phpunit": "^6.5",
    "phpunit/php-code-coverage": "^5.3",
    "phpstan/phpstan": "^0.9.1"
  },
  "require": {
    "php": ">=7.1.0",
    "phpoffice/phpspreadsheet": "^1.5.1"
  },
  "autoload": {
    "psr-4": {
      "FFQP\\": "src"
    },
    "classmap": [
      "src/"
    ],
    "exclude-from-classmap": [
      "/tests/**/*.php"
    ]
  },
  "scripts": {
    "analysis": "./vendor/bin/robo analysis",
    "coverage": "./vendor/bin/robo coverage:open",
    "test": "./vendor/bin/robo test"
  }
}
